@model Category

<div class="card" style="width: 25rem;">
    <div class="card-body">
        <h5 class="card-title">Category Name: @Model.Name</h5>
        <h5 class="card-title">Category Description: @Model.Description</h5>
        <h6 class="card-subtitle mb-2 text-muted">Department Id: @Model.CategoryId</h6>
        <a href="~/Category/Index" class="btn btn-primary mt-3">Back</a>
    </div>
</div>
